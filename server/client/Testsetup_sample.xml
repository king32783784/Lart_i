<?xml version="1.0" encoding="utf-8"?>

<test ostype="server"> 
  <list> 
    <info> 
      <summary>This is a list system update test requirements.</summary>  
      <osversion>4.0</osversion>  
      <arch>x86_64</arch>  
      <date>2016-04-13</date> 
    </info>  
    <import>
      <testtoolurl>http://192.168.32.18/testtool/</testtoolurl>
      <testquery>yes</testquery>
      <!--Whether other tests are carried out after the test is finished--> 
      <testtype>default</testtype>  
      <!-- default:Default test type custom:Custom test type-->  
      <default>performance</default>  
      <!--Specify default test type-->  
      <defperf>Perf_cpu</defperf>  
      <!--Default performance test items-->  
      <definfo>Hwinfo</definfo>  
      <!--Default info test items-->  
      <defstab>Stb_cpu</defstab>  
      <!--Default stability test items-->  
      <deffun>Fun_kernel</deffun>  
      <!--Default function test items-->  
      <cusperf>Perf_cpu</cusperf>  
      <!--Custom performance test items-->  
      <cusinfo>Hwinfo</cusinfo>  
      <!--Custom info test items-->  
      <cusstb>Stb_cpu</cusstb>  
      <!--Custom stability test items-->  
      <cusfun>Fun_kernel</cusfun>  
      <!--Custom function test items --> 
      <setuptype> testtoolurl testquery testtype default defperf definfo defstab deffun cusperf cusinfo cusstb cusfun</setuptype>
    </import>  
    <description> 
      <supporttype>info performance stability function</supporttype>  
      <!--Supporttype:Supported test types-->  
      <perfsupportlists>Perf_kernel Perf_cpu Perf_thread Perf_mem Perf_io Perf_net Perf_os</perfsupportlists>  
      <!--Perfsupportlists:Supported performance test items-->  
      <stbsupportlists>Perf_cpu......</stbsupportlists>  
      <!--stbsupportlists:Supported stability test items-->  
      <infosupportlists>Hwinfo Softinfo</infosupportlists>  
      <!--infosupportlists:Supported info test items-->  
      <stbsupportlists>Stb_cpu......</stbsupportlists>  
      <!--stbsupportlists:Supported stbility test items-->  
      <funsupportlists>Fun_kernel</funsupportlists>  
      <!--funsupportlists:Supported function test items--> 
    </description> 
  </list> 
</test>
