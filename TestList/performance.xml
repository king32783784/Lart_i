<?xml version="1.0"?>
<!--[Lart-i Performance test suite ]-->
<Perftestsuite>
  <SuiteInformation>
    <Title>Performance test Suite</Title>
    <Version>1.0.0</Version>
    <TestType>Performane</TestType>
    <Description>This test suite for Linux systems performance evaluation.</Description>
    <Maintainer>isoft_lp</Maintainer>
  </SuiteInformation>
  <Execute>
    <Test>Perf_kernel</Test>
    <Arguments>'lmbench',iterations=3</Arguments>
    <Description>lmbench</Description>
  </Execute>
  <Execute>
    <Test>Perf_cpu</Test>
    <Arguments>'sysbench', cpu_max_prime=[10000,20000,30000], test_type='cpu', runtimes=3, tag='cpu'</Arguments>
    <Description>sysbench</Description>
  </Execute>
  <Execute>
    <Test>Perf_mem</Test>
    <Arguments>'sysbench', num_threads=[4,8], test_type='memory', block_size='4096',total_size='8G', runtimes=3, tag='cpu'</Arguments>
    <Description>sysbench</Description>
  </Execute>
  <Execute>
    <Test>Perf_thread</Test>
    <Arguments>'sysbench', cpu_max_prime=[10000,20000,30000], test_type='cpu', runtimes=3, tag='cpu'</Arguments>
